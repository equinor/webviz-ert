QUEUE_SYSTEM LOCAL
QUEUE_OPTION LOCAL MAX_RUNNING 50

RUNPATH spe1_out/realization-%d/iter-%d

NUM_REALIZATIONS 2

ECLBASE SPE1CASE2
REFCASE <CONFIG_PATH>/resources/refcase/SPE1CASE2

SUMMARY WOPT:PROD WWPT:PROD WGPT:PROD WWIT:INJ
OBS_CONFIG obs.txt

GEN_KW FIELD_PROPERTIES field_properties.tmpl field_properties.json field_properties_priors

FORWARD_MODEL TEMPLATE_RENDER(<INPUT_FILES>=parameters.json, <TEMPLATE_FILE>=<CONFIG_PATH>/resources/SPE1CASE2.DATA.jinja2, <OUTPUT_FILE>=SPE1CASE2.DATA)

FORWARD_MODEL ECLIPSE100(<VERSION>=2022.2)
